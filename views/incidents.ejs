<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <title>IOW ICT Incidents</title>

    <link rel="canonical" href="https://getbootstrap.com/docs/4.5/examples/album/" />

    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"
        integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous" />

    <link href="css/album.css" rel="stylesheet" />
</head>

<body>
    <%- include('common/header'); -%>
        <div class="container">

            <% if (message) { %>
                <div class="row justify-content-md-center">
                    <div class="alert alert-primary" role="alert">
                        <%= message %>
                    </div>
                </div>

                <% } %>

                    <h1 class="text-justify">  Incidents  </h1>
                    <div class="col-md-12 col-md-offset-2  justify-content-center">
                        <!-- <a role="button" class="btn btn-primary btn-xs" href="/create-taster">Create a new taster</a> -->

                    </div>

                    <div class="col-md-12 col-md-offset-2">
                    
                        <table class="table table-striped custab">
                            <thead>
                                <tr>
                    
                                    <th>Incident Number</th>
                                    <th>Created Date</th>
                                    <th>Subject</th>
                                    <th>First Call Resolution</th>
                                    <th>Resolved Date</th>
                                    <th>Name</th>
                    
                    
                                </tr>
                                <% incidents.forEach(incident=> {
                    
                                    %>
                    
                                    <tr>
                    
                                        <td> <%= incident.IncidentNumber %></td>
                                        <td>
                                            <%= incident.CreatedDate %>
                                        </td>
                                        <td>
                                            <%= incident.Subject %>
                                        </td>
                                        <td>
                                            <%= incident.FirstCallResolution %>
                                        </td>
                                        <td>
                                            <%= incident.ResolvedDate %>
                                        </td>
                                        <td>
                                            <%= incident.Name %>
                                        </td>
                    
                                        
                    
                                        <% }); %>
                            </thead>
                    
                        </table>
                    
                    
                    </div>


                    <%- include('common/footer') -%>
</body>

</html>